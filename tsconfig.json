{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "src/components/Navbar.jsx",
    "src/app/layout.js",
    "src/app/page.js",
    "src/app/resume/page.js",
    "src/app/projects/page.js"
, "src/app/contact/page.js", "src/app/achievements/page.js", "src/app/about/page.js", "src/components/Footer.jsx", "src/components/Header.jsx", "src/components/PdfComp.jsx", "src/components/ProjectCard.jsx", "src/components/TypewriterHeading.jsx"  ],
  "exclude": ["node_modules"]
}
